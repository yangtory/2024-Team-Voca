//- extends ../layout

block content 
    link(rel="stylesheet", href="/css/commu/community.css")
    script(src="/js/commu/community.js")
    body
        div.back        
            div.mainList
                form(method ="POST") 
                    each item in VOCAS
                        div.element.item(data-v_seq= `${item.v_seq}`)
                            div.id.item.a(data-v_seq= `${item.v_seq}`)
                                img.item.a(src="/images/usericon.png", alt="프로필사진" width="50px")
                                span.item.a=item.v_멤버.m_id
                            span.item.a=item.v_name
                            
                            h4.item.a 추천수 
                                span.item.a=item.v_rec
                            div.item.a(data-v_seq= `${item.v_seq}`)
                                input(type="button" value="추천").like
                                
                    
                    


                        
                    